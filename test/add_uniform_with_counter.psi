        cur := uniform(0,1);
        x = x + cur;
        if cur<1/2 { y+=1; }
    }
    //z:=x==y;
    observe(y==1);
    return (x); // expected: -4·[-3/2+x≤0]·[-x+1≤0]·x+-[-1+x≤0]·[-x+1/2≤0]+-[-1+x≤0]·[-x+1/2≤0]·[-x+1≠0]+2·[-1+x≤0]·[-x+1/2≤0]·[-x+1≠0]·x+2·[-1+x≤0]·[-x+1/2≤0]·x+5·[-3/2+x≤0]·[-x+1≤0]+[-3/2+x≤0]·[-x+1≠0]·[-x+1≤0]
}
