                    gq1 := gq1i â‰  0;
                    gq2 := gq2i â‰  0;
                    r := measure(test3(c1, gc, gq1, gq2, H(0:ğ”¹)));
                    e := if c1 {
                        if gq1 { 1 } else { if gq2 { 3 } else { 0 } }
                    } else {
                        if gc { 2 } else { if gq2 { 3 } else { 0 } }
                    };
                    assert(r[0] = e);
                }
            }
        }
    }

    for ci in 0..2 {
        for gc1i in 0..2 {
            for gc2i in 0..2 {
                for gq1i in 0..2 {
                    for gq2i in 0..2 {
                        c   := ci â‰  0;
                        gc1 := gc1i â‰  0;
                        gc2 := gc2i â‰  0;
                        gq1 := gq1i â‰  0;
                        gq2 := gq2i â‰  0;
                        r := measure(test4(c, gc1, gc2, gq1, gq2, H(0:ğ”¹)));
                        e := if c {
                            if gq1 { 1 }
                            else { if gc1 { 2 }
                            else { if gq2 { 3 }
                            else { if gc2 { 4 }
                            else { 0 } } } }
                        } else { 0 };
                        assert(r[0] = e);
                    }
                }
            }
        }
    }

    for c1i in 0..2 {
        for c2i in 0..2 {
            for c3i in 0..2 {
                for gci in 0..2 {
                    for gq1i in 0..2 {
                        for gq2i in 0..2 {
                            c1  := c1i â‰  0;
                            c2  := c2i â‰  0;
                            c3  := c3i â‰  0;
                            gc  := gci â‰  0;
                            gq1 := gq1i â‰  0;
                            gq2 := gq2i â‰  0;
                            r := measure(test5(c1, c2, c3, gc, gq1, gq2, H(0:ğ”¹)));
                            e := if c1 {
                                if gq1 { 1 }
                                else { if c2 {
                                    if gc { 2 }
                                    else { if c3 {
                                        if gq2 { 3 } else { 4 }
                                    } else { 4 } }
                                } else { 4 } }
                            } else { 0 };
                            assert(r[0] = e);
                        }
                    }
                }
            }
        }
    }

    for c1i in 0..2 {
        for c2i in 0..2 {
            for gc1i in 0..2 {
                for gc2i in 0..2 {
                    for gq1i in 0..2 {
                        for gq2i in 0..2 {
                            c1  := c1i â‰  0;
                            c2  := c2i â‰  0;
                            gc1 := gc1i â‰  0;
                            gc2 := gc2i â‰  0;
                            gq1 := gq1i â‰  0;
                            gq2 := gq2i â‰  0;
                            r := measure(test6(c1, c2, gc1, gc2, gq1, gq2, H(0:ğ”¹)));
                            e := if c1 {
