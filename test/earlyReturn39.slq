def test6(const c1:ğ”¹, const c2:ğ”¹, gc1:!ğ”¹, gc2:!ğ”¹, const gq1:ğ”¹, const gq2:ğ”¹, q:ğ”¹) wild {
    x := 0:ğ”¹;
    if c1 {
        x = H(x);
        if gq1 { return (1:uint[3], q, x); }
        x := H(x);
        if gc1 { return (2:uint[3], q, x); }
    } else {
        if c2 {
            x = H(x);
            if gc2 { return (3:uint[3], q, x); }
            x := H(x);
            if gq2 { return (4:uint[3], q, x); }
        }
    }
    return (0:uint[3], q, x);
}


def main() {
    for ci in 0..2 {
        for gci in 0..2 {
            for gqi in 0..2 {
                c  := ci â‰  0;
                gc := gci â‰  0;
                gq := gqi â‰  0;
                r := measure(test1(c, gc, gq, H(0:ğ”¹)));
                e := if c { if gq { 1 } else { if gc { 2 } else { 0 } } } else { 0 };
                assert(r[0] = e);
            }
        }
    }

    for c1i in 0..2 {
        for c2i in 0..2 {
            for gci in 0..2 {
                for gq1i in 0..2 {
                    for gq2i in 0..2 {
                        c1  := c1i â‰  0;
                        c2  := c2i â‰  0;
                        gc  := gci â‰  0;
                        gq1 := gq1i â‰  0;
                        gq2 := gq2i â‰  0;
                        r := measure(test2(c1, c2, gc, gq1, gq2, H(0:ğ”¹)));
                        e := if c1 {
                            if gq1 { 1 }
                            else { if c2 {
                                if gc { 2 }
                                else { if gq2 { 3 } else { 4 } }
                            } else { 4 } }
