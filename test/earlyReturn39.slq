
def test1(const c:ğ”¹, gc:!ğ”¹, const gq:ğ”¹, q:ğ”¹) wild {
    x := 0:ğ”¹;
    if c {
        x := H(x);
        if gq { return (1:uint[3], q, x); }
        x := H(x);
        if gc { return (2:uint[3], q, x); }
    }
    return (0:uint[3], q, x);
}

def test2(const c1:ğ”¹, const c2:ğ”¹, gc:!ğ”¹, const gq1:ğ”¹, const gq2:ğ”¹, q:ğ”¹) wild {
    x := H(0:ğ”¹);
    if c1 {
        if gq1 { return (1:uint[3], q, x, 0:ğ”¹); }
        y := 0:ğ”¹;
        if c2 {
            y := H(y);
            x := H(x);
            if gc { return (2:uint[3], q, x, y); }
            if gq2 { return (3:uint[3], q, x, y); }
        }
        return (4:uint[3], q, x, y);
    }
    return (0:uint[3], q, x, 0:ğ”¹);
}

def test3(const c1:ğ”¹, gc:!ğ”¹, const gq1:ğ”¹, const gq2:ğ”¹, q:ğ”¹) wild {
    x := 0:ğ”¹;
    if c1 {
        x = H(x);
        if gq1 { return (1:uint[3], q, x); }
        x := H(x);
    } else {
        if gc { return (2:uint[3], q, x); }
        x := H(x);
    }
    if gq2 { return (3:uint[3], q, x); }
    return (0:uint[3], q, x);
}

def test4(const c:ğ”¹, gc1:!ğ”¹, gc2:!ğ”¹, const gq1:ğ”¹, const gq2:ğ”¹, q:ğ”¹) wild {
