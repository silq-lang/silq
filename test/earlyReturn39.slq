def test5(const c1:ğ”¹, const c2:ğ”¹, const c3:ğ”¹, gc:!ğ”¹, const gq1:ğ”¹, const gq2:ğ”¹, q:ğ”¹) wild {
    x := 0:ğ”¹;
    if c1 {
        x = H(x);
        if gq1 { return (1:uint[3], q, x, 0:ğ”¹); }
        y := 0:ğ”¹;
        if c2 {
