def partition[n:!â„•](xs: !â„•^n,l:!â„•,r:!â„•,p:!â„•){
	ys:=xs;
	q:=p;
	(ys[p],ys[r])=(ys[r],ys[p]);
	(i,j):=(l,r-1);
	done:=false;
	while !done{
		while iâ‰¤j && ys[i]<ys[r]{ i+=1; }
		while iâ‰¤j && ys[j]â‰¥ys[r]{ j-=1; }
		if i>j { (ys[i],ys[r])=(ys[r],ys[i]); q=i; done=true; }
		else { (ys[i],ys[j])=(ys[j],ys[i]); i+=1, j-=1; }
	}
	return (ys,q);
}
