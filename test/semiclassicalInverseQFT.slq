
def QFT[n:!](: int[n])mfree: int[n]{
	for k in [0..n div 2){
		([k],[n-k-1]) := ([n-k-1],[k]);
	}
	for k in [0..n){
		[k] := H([k]);
		for l in [k+1..n){
			if [k] && [l]{
				phase(2路路2^(k-l-1));
			}
		}
	}
	return ;
}

def sciQFT[n:!](:int[n]){
	:= as ^n;
	r:=();
    for k in (n..-1..0]{
        for l in (n..-1..k + 1]{
            if r[n-1-l] && [k] {
                phase(-2路路2^(k-l-1));
            }
        }
		~[t]:=;
		r~=[measure(H(t))];
    }
	():=;
	return r coerce !^n as !int[n];
}	

n:=5;
def main(){
	x:=0:int[n];
	for i in 0..n{ x[i]:=H(x[i]); }
	x:=measure(x);
	assert(sciQFT(QFT(x))==x);
	return x;
}
