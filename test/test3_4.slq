def a4_HADAMARD_Array[k:!N](q:ğ”¹^k) : ğ”¹^k {
	for j in [0..k) { q[j] := H(q[j]); }
	return q;
}

def a4_HADAMARD_Array_Array[k:!N,l:!N](q:(ğ”¹^k)^l) : (ğ”¹^k)^l {
	for i in [0..l) {
		q[i] := a4_HADAMARD_Array(q[i]);
	}
	return q;
}

//def a5_SETUP(oracle:!QWTFP_spec, tt:const Node[]) : ğ”¹[][] {
def a5_SETUP[n:!N, rr:!N](edgeOracle:((const int[n] x const int[n] x ğ”¹) !-> mfree ğ”¹), 
	const tt:int[n]^rr) : (ğ”¹^rr)^rr {

	ee := vector(rr,vector(rr,false)):(B^rr)^rr;

	for k in [0..rr) {
		for j in [0..k) {
			ee[k][j] := edgeOracle(tt[j], tt[k], ee[k][j]);
	}	}

	return ee;
}

