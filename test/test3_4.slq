def a6_QWSH[n:!N, r:!N, rr:!N](edgeOracle:((const int[n] x const int[n] x ğ”¹) !-> mfree ğ”¹), 
	tt:int[n]^rr, i:int[r], v:int[n], 
	ee:(ğ”¹^rr)^rr) : int[n]^rr x int[r] x int[n] x (ğ”¹^rr)^rr {

	//todo check if capturing here is enough
	f := lambda (const i:int[r], tt:int[n]^rr, ee:(ğ”¹^rr)^rr) mfree . {
		eed := vector(rr, false:B); 
		ttd := tt[i]; 
		(ee2, eed2) := a12_FetchStoreE(i, ee, eed);
		eed3 := a13_UPDATE(edgeOracle, tt, ttd, eed2);
		tt2 := a9_StoreT_Array(i, tt, ttd);
		
		return (ttd, ee2, eed3, tt2);
	};
	
	(i, v) := a7_Diffuse_Pair(i, v);
	(ttd,ee4,eed4,tt4) := f(i,tt,ee);
	(ttd, v) := (v, ttd);
	(tt5,ee5) := reverse(f)(i,(ttd,ee4,eed4,tt4));

	return (tt5, i, v, ee5);
}


def a7_Diffuse_Array[k:!N](q:ğ”¹^k) : ğ”¹^k {
	q := a4_HADAMARD_Array(q);
	if q == array(k,false) { phase(Ï€); }
	q := a4_HADAMARD_Array(q);
	return q;
}

def a7_Diffuse_Array_Array[k:!N,l:!N](q:(ğ”¹^k)^l) : (ğ”¹^k)^l {
	q := a4_HADAMARD_Array_Array(q);
	if q == array(l,array(k,false)) { phase(Ï€); }
	q := a4_HADAMARD_Array_Array(q);
	return q;
}

def a7_Diffuse_Pair[k:!N, l:!N](p:int[k], q:int[l]) : int[k] x int[l] {
	p := a4_HADAMARD_Array(p as B^k) as int[k];
	q := a4_HADAMARD_Array(q as B^l) as int[l];
	if q == 0 && p==0 { phase(Ï€); }
	p := a4_HADAMARD_Array(p as B^k) as int[k];
	q := a4_HADAMARD_Array(q as B^l) as int[l];
	return (p,q);
}


def flipWith_Array[l:!N](const p:ğ”¹^l, q:ğ”¹^l) mfree : ğ”¹^l {
	for i in[0..l) {
		if p[i] { q[i] := X(q[i]); }
	}
	return q;
}

