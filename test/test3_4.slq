def a8_FetchT[n:!N, rr:!N, r:!N](const i:int[r], const tt:ğ”¹^rr) :  ğ”¹ {
	ttd := false:B;
	for j in [0..rr) {
		if tt[j] && i == j {
			ttd_ := !ttd;
			forget(ttd = !ttd_);
			ttd := ttd_;
	}	}	
	return ttd;
}

