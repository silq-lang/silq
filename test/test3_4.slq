def help_a20_2[n:!N, r:!N, rr:!N, rbar:!N, rrbar:!N](
	edgeOracle:((const int[n] x const int[n] x ğ”¹) !-> mfree ğ”¹), 
	const w:int[n], const iota:int[rbar], const tt:int[n]^rr, const ee:(ğ”¹^rr)^rr, 
	tau:int[r]^rrbar, eew:ğ”¹^rrbar, cTri:int[rrbar]) :
	int[r]^rrbar x ğ”¹^rrbar x int[rrbar] x int[r] x B {

	eewd := false:B;

	taud := tau[iota]; 
	(eew, eewd) := a10_FetchStoreT(iota, eew, eewd);

	for k in [0..rrbar) {
		if ee[taud][tau[k]] && eewd && eew[k] {
			cTri -= 1;
	}	}

	eewd := edgeOracle(tt[taud], w, eewd);
	tau := a9_StoreT_Array(iota, tau, taud);

	return (tau, eew, cTri, taud, eewd);
}
