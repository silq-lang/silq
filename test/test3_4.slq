def a9_StoreT_Array[n:!N, rr:!N, r:!N](const i:int[r], tt: int[n]^rr, const ttd:int[n]) mfree : int[n]^rr {
	for j in [0..rr) {
		if i==j {
			tt[j] := flipWith_Array(ttd as B^n, tt[j] as B^n) as int[n];
	}	}
	return tt;
}

// def a10_FetchStoreT[k:!Int](i:const Int[k], tt:ğ”¹[], ttd:ğ”¹) : ğ”¹[] x ğ”¹ {
def a10_FetchStoreT[rr:!N, r:!N](const i:int[r], tt:B^rr, ttd:ğ”¹) : ğ”¹^rr x ğ”¹ {
	for j in [0..rr) {
		if i == j {
			(tt[j], ttd) := (ttd, tt[j]);
		}
	}
	return (tt, ttd);
}

// todo: check if ok!
def a11_FetchE[rr:!N,r:!N](const i:int[r], const qs:(ğ”¹^rr)^rr) lifted : ğ”¹^rr {
	ps := vector(rr,false:ğ”¹);
	for j in [0..rr) {
		for k in [0..j) {
			if qs[j][k] && i == j { ps[k] = !ps[k]; }
			if qs[j][k] && i == k { ps[j] = !ps[j]; }
	}	}
	return ps;
}


def a12_FetchStoreE[rr:!N,r:!N](const i:int[r], qs: (ğ”¹^rr)^rr, ps: ğ”¹^rr) mfree : (ğ”¹^rr)^rr x ğ”¹^rr {

	for j in [0..rr) {
		for l in [0..j) {
			if i == j { (qs[j][l], ps[l]) := (ps[l], qs[j][l]); }
			if i == l { (qs[j][l], ps[j]) := (ps[j], qs[j][l]); }
		}
	}
	return (qs, ps);
}


