// args: --run

arrayLength := 3;

def map(f: !â„ !â†’ !â„){
    return (arr: !â„[]){
        result := [];
        for i in [0..arrayLength){
            result ~= [f(arr[i])];
        }
        return result;
    }
}

def all(f: !â„ !â†’ !ğ”¹){
    return (arr: !â„[]){
        result := true;
        for i in [0..arrayLength){
            result &&= f(arr[i]);
        }
        return result;
    }
}

def main(){
	def flip(x:!â„)â‡’true;
    g := map(flip);
    a := [1/2,1/3,1/4];
    h := (x:!â„)=>x!=0;
    r := all(h)(g(a));
	assert(r);
}
