def exp_branch(gc:!ğ”¹, gq:!ğ”¹, pat:!â„•, id1:!â„•, id2:!â„•) : !â„• {
	if pat = 0 { return 0; }
	if pat = 1 { if gc { return id1; } return 0; }
	if pat = 2 { if gq { return id1; } return 0; }
	if pat = 3 { if gc { return id1; } if gq { return id2; } return 0; }
	// pat = 4 (QC)
	if gq { return id1; }
	if gc { return id2; }
	return 0;
}

def exp(c:!ğ”¹, gc:!ğ”¹, gq:!ğ”¹, tp:!â„•, ep:!â„•) : !â„• {
	if c { return exp_branch(gc, gq, tp, 1, 2); }
	return exp_branch(gc, gq, ep, 3, 4);
}

def main() {
	for ci in 0..2 {
		for gci in 0..2 {
			for gqi in 0..2 {
				c  := ci â‰  0;
				gc := gci â‰  0;
				gq := gqi â‰  0;

				r := measure(test_N_N(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 0, 0));

				r = measure(test_N_C(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 0, 1));

				r = measure(test_N_Q(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 0, 2));

				r = measure(test_N_CQ(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 0, 3));

				r = measure(test_N_QC(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 0, 4));

				r = measure(test_C_N(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 1, 0));

				r = measure(test_C_C(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 1, 1));

				r = measure(test_C_Q(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 1, 2));

				r = measure(test_C_CQ(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 1, 3));

				r = measure(test_C_QC(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 1, 4));

				r = measure(test_Q_N(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 2, 0));

				r = measure(test_Q_C(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 2, 1));

				r = measure(test_Q_Q(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 2, 2));

				r = measure(test_Q_CQ(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 2, 3));

				r = measure(test_Q_QC(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 2, 4));

				r = measure(test_CQ_N(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 3, 0));

				r = measure(test_CQ_C(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 3, 1));

				r = measure(test_CQ_Q(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 3, 2));

				r = measure(test_CQ_CQ(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 3, 3));

				r = measure(test_CQ_QC(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 3, 4));

				r = measure(test_QC_N(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 4, 0));

				r = measure(test_QC_C(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 4, 1));

				r = measure(test_QC_Q(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 4, 2));

				r = measure(test_QC_CQ(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 4, 3));

				r = measure(test_QC_QC(c, gc, gq, H(0:ğ”¹)));
				assert(r[0] = exp(c, gc, gq, 4, 4));
			}
		}
	}
