	assert(measure((7:!Z) div (-3:int[4])) == -3);
	assert(measure((6:!Z) % (-3:int[4])) == 0);
	assert(measure((7:!Z) % (-3:int[4])) == -2);

	assert(measure((-6:!Z) div (3:int[3])) == -2);
