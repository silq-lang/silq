	assert(measure((6:!Z) % (3:uint[3])) == 0);
	assert(measure((7:!Z) % (3:uint[3])) == 1);

	// assert(measure((-6:!Z) div (3:uint[2])) == 2);
	// assert(measure((-7:!Z) div (3:uint[2])) == 1);
	assert(measure((-6:!Z) % (3:uint[2])) == 0);
	assert(measure((-7:!Z) % (3:uint[2])) == 2);

	// assert(measure((-6:!Z) % (3:uint[3])) == 6);
	// assert(measure((-7:!Z) % (3:uint[3])) == 5);
	assert(measure((-6:!Z) % (3:uint[3])) == 0);
	assert(measure((-7:!Z) % (3:uint[3])) == 2);

	assert(measure((6:!N) div (-3:int[3])) == -2);
