
n:=3,m:=4;
def main(){
	x:=vector(n,vector(m,1));
	y:=vector(n,vector(m,0));
	for i in 0..n{
		for j in 0..m{
			for k in 0..4{
				ni:=i+(k=0)-(k=1);
				nj:=j+(k=2)-(k=3);
				if 0≤ni∧ni<n∧0≤nj∧nj<m{ // // TODO: chained comparisons
					y[i][j]+=x[ni][nj];
				}
			}
		}
	}
	assert(y=((2,3,3,2),(3,4,4,3),(2,3,3,2)));
	return y;
}
