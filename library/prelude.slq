def csqrt(z:!â„‚):!â„‚ {
	(x,y):=ctori(z);
	if y=0{
		if xâ‰¥0{ return cfromri(sqrt(x),0); }
		else{ return cfromri(0,sqrt(-x)); }
	}
	r:=hypot(x,y);
	if xâ‰¥0{
		u:=sqrt((r+x)/2);
		v:=y/(2Â·u);
		return cfromri(u,v);
	}else{
		v:=sign(y)Â·sqrt((r-x)/2);
		u:=y/(2Â·v);
		return cfromri(u,v);
	}
}
def csqrt1pm1(z:!â„‚)lifted:!â„‚{
	if z = 0 { return 0; }
